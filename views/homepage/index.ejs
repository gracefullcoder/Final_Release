            class="shape shape-2 move-anim">

        </div>

      </section>


      <!-- Countdown Timer Section -->
      <section class="section countdown bg-black-10 text-center" aria-label="countdown" id="workshop">
        <div class="container">
          <p style="visibility: hidden;" class="gettime">
            <%=allSection.workshop[allSection.workshop.length - 1].time %>
          </p>
          <p class="section-subtitle label-2">
            <%=allSection.workshop[allSection.workshop.length - 1].label %>
          </p>

          <h2 class="headline-1 section-title" id="countdown-title">
            <%=allSection.workshop[allSection.workshop.length - 1].title %>
          </h2>

          <div id="countdown" class="countdown-container">
            <div class="countdown-item">
              <span class="countdown-number"></span>
              <span class="countdown-label">Days</span>
            </div>
            <div class="countdown-item">
              <span class="countdown-number"></span>
              <span class="countdown-label">Hours</span>
            </div>
            <div class="countdown-item">
              <span class="countdown-number"></span>
              <span class="countdown-label">Minutes</span>
            </div>
            <div class="countdown-item">
              <span class="countdown-number"></span>
              <span class="countdown-label">Seconds</span>
            </div>
          </div>

          <p class="section-text">
            <%=allSection.workshop[allSection.workshop.length - 1].text %>
          </p>

          <!-- Signup Form -->
          <form id="signupForm" class="signup-form"
            action="/register/<%=allSection.workshop[allSection.workshop.length - 1]._id%>" method="post">
            <label for="phonenumber">Phone Number:</label>
            <input type="text" id="phonenumber" name="phonenumber" required>

            <label for="message">Message:</label>
            <input type="text" id="message" name="userName" required>

            <% if(!allSection.isWorkshop){ %>
              <p class="btn btn-primary">
                <span class="text text-1">Registrations are Over!</span>
                <span class="text text-2" aria-hidden="true">Starts Soon!</span>
              </p>
              <%}else if(allSection.user) {%>
                <button type="submit" class="btn btn-primary">
                  <span class="text text-1">Register!</span>
                  <span class="text text-2" aria-hidden="true">Register!</span>
                </button>
                <%}else{ %>
                  <a href="/auth/signup" class="btn btn-primary">
                    <span class="text text-1">Sign in to Register!</span>
                    <span class="text text-2" aria-hidden="true">Sign in to Register!</span>
                  </a>
                  <% } %>
          </form>
        </div>
      </section>


      <!--- #ABOUT-->
      <section class="section about text-center" aria-labelledby="about-label" id="about">
        <div class="container about" style="max-width: 1400px;">
          <figure class="about-banner" id="slideshow-container">
            <!-- Main banner images -->
            <img class="banner-image" src="/assets/images/koe line.jpg" loading="lazy" alt="">
            <!-- <img class="banner-image" src="/assets/images/coffeeroaster.jpg" width="300" height="200" loading="lazy" alt=""> -->
            <!-- Koe logo -->

            <div class="abs-img abs-img-2 has-before">

              <img src="/assets/images/koe-logo.png" width="133" height="134" loading="lazy" alt="">
            </div>
          </figure>
          <div class="about-content">
            <p class="label-2 section-subtitle" id="about-label">Our Story</p>
            <h2 class="headline-1 section-title">Every Flavor Tells a Story</h2>
            <p class="section-text">
              <br />
              Bonjour Surat! Visit our café to experience the finest French cuisine and the allure of a French café.
              <br /> <br />
              Bringing to you, a <b>state-of-the-art Coffee Roastery</b>at Koe’s Rooftop Kafe! ☕️✨
              <br /> <br />
              The inception of Kôė was to create a micro scale coffee roasting and food curation experience, with a
              serene ambience to make the whole experience a complete package for the clientele.
            </p>
            <div class="contact-label">Book Through Call</div>
            <a href="tel:+91 96246 96846" class="body-1 contact-number hover-underline">+91 96246 96846</a>
          </div>
        </div>
      </section>

      <section class="section bread-and-cheese-variety text-center">
        <div class="bread-and-cheese">
          <div class="headline-2">
            Do check out our collection of breads and cheeses
          </div>
          <div class="combine-bread-and-cheese">
            <div class="bread">
              <a href="/assets/cheese and bread/bread.html" target="_blank" class="has-before">
                <img src="/assets/cheese and bread/bread_thumbnail.jpeg" width="246" height="452"
                  class="bread-and-cheese-img">
              </a>
              <div class="bread-heading headline-2">
                Breads
              </div>
            </div>
            <div class="cheese">
              <a href="/assets/cheese and bread/cheese.html" target="_blank" class="has-before ">
                <img src="/assets/cheese and bread/cheese_thumbnail.jpeg" width="246" height="212"
                  class="bread-and-cheese-img">
              </a>
              <div class="cheese-heading headline-2">
                Cheeses
              </div>
            </div>
          </div>
        </div>
      </section>

      <!--- #PAIR Cheese with coffee blends-->

      <section class="pairing-section">
        <div class="container">
          <h2 class="headline-1">Coffee and Cheese Pairing</h2>

          <div class="selector-container">
            <label for="coffeeSelector">Select a Coffee Blend:</label>
            <select id="coffeeSelector">
              <option value="mediumRoast" data-image="mediumRoast.jpg">Medium Roast</option>
              <option value="darkRoast" data-image="darkRoast.jpg">Dark Roast</option>
              <option value="flavouredCoffee" data-image="flavouredCoffee.jpg">Flavored Coffee</option>
            </select>
            <br />
            <img id="coffeeImage" src="" alt="Coffee Image" width="80%" height="200px">
          </div>

          <div class="selector-container">
            <label for="cheeseSelector">Select a Cheese:</label>
            <select id="cheeseSelector">
              <option value="brie" data-image="brie.jpg">Brie</option>
              <option value="gouda" data-image="gouda.jpg">Gouda</option>
              <option value="blue" data-image="blue.jpg">Blue Cheese</option>
            </select>
            <br />
            <img id="cheeseImage" src="" alt="Cheese Image" width="80%" height="300px">
          </div>
          <b><br />
            <div class="pairing-result-text hover:shine " id="pairingResult"
              style="color: white; border: 4px  dotted; border-color: black; border-radius: 5px;">
              <!-- Pairing result will be displayed here -->
            </div>
          </b>
        </div>
      </section>




      <!---#SPECIAL DISH -->

