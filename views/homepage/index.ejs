                    <div class="special-dish-content bg-black-10">
                      <div class="container">

                        <img src="/assets/images/badge-1.png" width="28" height="41" loading="lazy" alt="badge"
                          class="abs-img">

                        <p class="section-subtitle label-2">
                          <%=specialsection.label %>
                        </p>

                        <h2 class="headline-1 section-title">
                          <%=specialsection.title %>
                        </h2>

                        <p class="section-text">
                          <%=specialsection.text %>
                        </p>



                      </div>
                    </div>

                    <% if(index==1) { %>
                      <img src="https://ik.imagekit.io/vaibhav11/shape-9.png?updatedAt=1707734962408"
                        style="transform:translateY(15%) ;" width="351" height="462" loading="lazy" alt=""
                        class="shape shape-2">
