      <a href="tel:To add" class="topbar-item link">
        <div class="icon">
          <ion-icon name="call-outline" aria-hidden="true"></ion-icon>
        </div>

        <span class="span">+91 96246 96846</span>
      </a>

      <div class="separator"></div>

      <a href="mailto:koethekafe@gmail.com" class="topbar-item link">
        <div class="icon">
          <ion-icon name="mail-outline" aria-hidden="true"></ion-icon>
        </div>

        <span class="span">koethekafe@gmail.com</span>
      </a>

    </div>
  </div>

  <header class="header" data-header>
    <div class="container">

      <a href="#top" class="logo">
        <img src="/assets/images/koe-logo.png" alt="Koe logo" class="logo-img">
      </a>

      <nav class="navbar" data-navbar>

        <button class="close-btn" aria-label="close menu" data-nav-toggler>
          <ion-icon name="close-outline" aria-hidden="true"></ion-icon>
        </button>

        <a href="#" class="logo">
          <img src="/assets/images/koe-logo.png" width="160" height="50" alt="Koe logo">
        </a>

        <ul class="navbar-list">

          <li class="navbar-item">
            <a href="#home" class="navbar-link hover-underline active">
              <div class="separator"></div>

              <span class="span">Home</span>
            </a>
          </li>

          <li class="navbar-item">
            <a href="/assets/KoeCafeFood.pdf" class="navbar-link hover-underline" target="_blank">
              <div class="separator"></div>

              <span class="span">Menus</span>
            </a>
          </li>

          <li class="navbar-item">
            <a href="#about" class="navbar-link hover-underline">
              <div class="separator"></div>

              <span class="span">About Us</span>
            </a>
          </li>

          <li class="navbar-item">
            <a href="#workshop" class="navbar-link hover-underline">
              <div class="separator"></div>

              <span class="span">Workshop</span>
            </a>
          </li>

        </ul>

        <div class="text-center">
          <p class="headline-1 navbar-title">Visit Us</p>

          <address class="body-4">
            205, International Finance Centre, VIP Road, opp. Fire Station, Vesu, <br>
            Surat, Gujarat 395007
          </address>

          <p class="body-4 navbar-text">Open: 12.00 pm to 12.30 am</p>

          <a href="mailto:koethecafe@gmail.com" class="body-4 sidebar-link">koethecafe@gmail.com</a>

          <div class="separator"></div>

          <p class="contact-label">Booking Request</p>

          <a href="tel:+91 96246 96846" class="body-1 contact-number hover-underline">
            +91 96246 96846
          </a>
        </div>

      </nav>


      <div style="display: flex;align-items: center;">
        <%if(!allSection.user){%>
          <a href="/auth/signup" class="btn btn-secondary">
            <span class="text text-1">Login</span>

            <span class="text text-2" aria-hidden="true">Sign Up!</span>
          </a>
          <a href="/auth/signup" class="signin-text">Sign in</a>
          <%} else{%>
            <div class="notification-badge-div">
              <% if(allSection.user.notificationRemaining){ %>
                <div class="notification-badge"></div>
                <% } %>
                  <img id="profilepicture" src="<%=allSection.user.profilepicture.imagelink%>" alt="DP">
            </div>

            <div id="account-options" class="account-options">
              <div class="account-field-name">Hi <%=allSection.user.fullname%>!</div>
              <div class="account-field">
                <a href="/dashboard?section=dashboard" class="account-field-link">
                  <div class="separator"></div>
                  <span class="span">Account Details</span>
                </a>
              </div>
              <div class="account-field">
                <a href="/dashboard?section=notifications" class="account-field-link">
                  <div class="separator"></div>
                  <div style="display: flex;">
                    <span class="span">
                      Notifications
                    </span>
                    <% if(allSection.user.notificationRemaining){ %>
                      <p style="color: red;">
                        + <%= allSection.user.notificationRemaining %>
                      </p>
                      <% } %>
                  </div>


                </a>
              </div>
              <div class="account-field">
                <a href="/dashboard?section=activity" class="account-field-link">
                  <div class="separator"></div>
                  <span class="span">Activity</span>
                </a>
              </div>
              <% if(allSection.user.role.admin){ %>
                <div class="account-field">
                  <a href="/admin" class="account-field-link">
                    <div class="separator"></div>
                    <span>Admin Features 💼</span>
                  </a>
                </div>
                <% } %>
                  <div class="account-field">
                    <a href="/auth/logout" class="account-field-link">
                      <div class="separator"></div>
                      <span>Logout</span>
                    </a>
                  </div>
            </div>
            <%}%>

              <button class="nav-open-btn" aria-label="open menu" data-nav-toggler>
                <span class="line line-1"></span>
                <span class="line line-2"></span>
                <span class="line line-3"></span>
              </button>
      </div>


      <div class="overlay" data-nav-toggler data-overlay></div>

    </div>
  </header>

  <main>
    <article>

      <section class="hero text-center" aria-label="home" id="home">

        <ul class="hero-slider" data-hero-slider>
          <% for(heroSlider of allSection.heroSliders) {%>
            <li class="slider-item active" data-hero-slider-item>

              <div class="slider-bg">
                <img src=<%=heroSlider.image%> width="1880" height="950" alt=""
                class="img-cover">
              </div>

              <p class="label-2 section-subtitle slider-reveal">
                <%=heroSlider.label%>
              </p>

              <h1 class="display-1 hero-title slider-reveal">
                <%=heroSlider.title%>
              </h1>

              <p class="body-2 hero-text slider-reveal">
                <%=heroSlider.text%>.
              </p>

              <a href="/assets/KoeCafeFood.pdf" class="btn btn-primary slider-reveal" target="_blank">
                <span class="text text-1">View Our Menu</span>

                <span class="text text-2" aria-hidden="true">View Our Menu</span>
              </a>

            </li>
            <%}%>

              

        </ul>

        <button class="slider-btn prev" aria-label="slide to previous" data-prev-btn style="left: 5%;">
          <ion-icon name="chevron-back"></ion-icon>
        </button>

        <button class="slider-btn next" aria-label="slide to next" data-next-btn style="right: 5% ;">
          <ion-icon name="chevron-forward"></ion-icon>
        </button>

        <a href="#reservation" class="hero-btn has-after">
          <img src="/assets/images/hero-icon.png" width="48" height="48" alt="booking icon">

          <span class="label-2 text-center span">Book A Table</span>
        </a>

      </section>

      <!--- #SERVICE-->

